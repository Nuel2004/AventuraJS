{
  "source": {
    "include": [
      "./Modules", 
      "./script.js",
      "./utils"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "template": "./node_modules/jsdoc/templates/default"
  }
}
